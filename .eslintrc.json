{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "config": true,
    "Stats": true,
    "ga": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {},
    "sourceType": "module"
  },
  "extends": [
    "vizzuality"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
